/* 顶部问题 */
.index-question{
  display: block;
  width: 100%;
  height: 500rpx;
  position: relative;
  padding-top: 1px;
  border-bottom:12rpx solid #f4f4f4;
  background: url("data:image/jpg;base64,") no-repeat center top;
  background-size: 100% 100%;
}
.index-question-font{
  display: block;
  width: 142rpx;
  margin: 40rpx 0 0 30rpx;
}
.index-question-content{
  display: block;
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  font-weight: bold;
  color:#fff;
}
.index-question-answer{
  position: absolute;
  top:220rpx;
  left:30rpx;
  width: 600rpx;
}
.index-question-answer-list{
  font-size: 28rpx;
  color:#fff;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.index-question-answer-list text{
  display: inline-block;
  color:#000;
  padding: 0 24rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #fff;
  border-radius: 10rpx;
}
.index-question-count{
  position: absolute;
  bottom:20rpx;
  right:30rpx;
  color:#fff;
  font-size: 24rpx;
  font-weight: bold;
}
/* 商品分类swiper */
.index-list-swiper{
  display: block;
  width: 690rpx;
  height: 200rpx;
  padding: 30rpx 0;
  margin: 0 auto;
}
.index-list-swiper-item{
  display: block;
  width: 690rpx;
  /* height: 100%; */
  overflow: hidden;
  padding: 30rpx 0;
}
.list-swiper-item-left{
  display: block;
  float: left;
  width: 460rpx;
}
.goods-title{
  display: block;
  width: 390rpx;
  overflow: hidden;
  max-height: 80rpx;
  padding: 20rpx 0; 
  line-height: 40rpx;
  font-size: 25rpx;
  margin-left: 30rpx;
}
.goods-price{
  display: block;
  width: 390rpx;
  overflow: hidden;
  height: 80rpx;
  font-size: 32rpx;
  font-weight: bolder;
  line-height: 80rpx;
  margin-left: 30rpx;
}
.list-swiper-item-right{
  display: block;
  float: left;
  width: 196rpx;
  height: 196rpx;
  border: 2rpx solid #f4f4f4;
  background:#f4f4f4; 
}
.index-list-swiper-line{
  display: block;
  width: 100%;
  height: 1px;
  background: #f4f4f4;
}
.list-swiper-left,.list-swiper-right{
  position: absolute;
  top:50%;
  width: 10px;
  height: 17.15px;
  margin-top: -8px;
}
.list-swiper-left{
  left:20rpx;
}
.list-swiper-right{
  right:20rpx;
}


.recommend-tj{
  display: block;
  width: 290rpx;
  margin: 35rpx auto;
}

/* 没有商品列表时显示的盒子 */
.recommend-list-none{
  display: block;
  width: 100%;
  background: #f7f8ff;
  padding-top:1px;
  box-sizing: border-box;
}
.recommend-list-none image{
  display: block;
  width: 239rpx;
  height: 180rpx;
  margin: 150rpx auto 10rpx;
}
.recommend-list-none text{
  display: block;
  width: 100%;
  font-size: 32rpx;
  color:#afb1c0;
  font-weight: bold;
  text-align: center;
}

/* 测试完毕 */
.test-over{
  display: block;
  width: 100%;
  margin: 150rpx auto 54rpx;
  text-align: center;
  font-size: 48rpx;
  color:#c49a58;
  padding-left: 38rpx;
  box-sizing: border-box;
}
.test-reload-btn{
  display: block;
  width: 256rpx;
  height:80rpx;
  margin: 0 auto;
}


/* 回到顶部 */
.backtop{
  display: block;
  width: 60rpx;
  height: 60rpx;
  position: fixed;
  bottom:12rpx;
  right:12rpx;
}

/* 返回上一题按钮 */
.recommend-back-prev{
  position: absolute;
  top:35rpx;
  right:30rpx;
  width: 40rpx;
  height: 40rpx;
}