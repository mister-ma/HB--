.container{
  width: 100%;
  display: block;
}
/* pages/component/index/article-detail.wxss */
.page-body{
  background: #fff;
  width: 100%;
  /* padding: 0 15px; */
  box-sizing: border-box;
  position: relative;
  padding-bottom: 55px;
}
.page-body.ipone{
  padding-bottom: 70px;
}
/*文章标题*/
.article-title{
  color: #1E1E1E;
  width: auto;
  font-weight: 700;
  font-size: 20px;
  margin: 5px 15px 15px;
}
/*编辑信息*/
.author-createtime-box{
  display: block;
  overflow: hidden;
  height: 40px;
  margin: 0 15px;
}
.author-avatar{
  display:inline-block;
  vertical-align: middle;
  height:32px;
  position: relative;
  width: 32px;
}
.author-avatar-img{
  width: 32px;
  border-radius: 16px;
  height:32px;
  /* border: 1px solid #000;  */
  vertical-align: middle;
  display:inline-block;
}
.author-avatar-p{
  position: absolute;
  top:0;
  right:-5.5px;
  width: 15px;
  height:15px;
  display:inline-block;
}
.author-nickname{
  width: 220px;
  line-height: 32px;
  vertical-align: middle;
  height: 32px;
  display: inline-block;
  font-size: 10px;
  margin-left: 15px;
}
.author-nickname-name{
  font-size: 12px;
  font-weight: 700;
}
.author-nickname-line{
  margin: 0 8px;
  display: inline-block;
  height: 20px;
}
/*创建时间*/
.create-time{
  display: block;
  float: right;
  font-size: 10px;
  height: 32px;
  line-height: 32px;
}
.author-left{
  display: block;
  float: left;
  height: 40px;
}
/*商品模板样式*/
.article-content-goods{
  display: block;
  padding-bottom: 15px;
  border-bottom:1px solid #d2d2d2;
  margin: 0 15px 20px;
  clear:both;
}
.article-content-goods-img{
  display: block;
  width: 100%;
  margin-bottom: 10px;
}
.article-content-goods-brand,.article-content-goods-name,.article-content-goods-price{
  display: block;
  position: relative;
  z-index: 10;
  left: 0;
  top:0;
  width:100%;
  font-size: 14px;
  text-align: left;
  line-height: 20px;
  color: #1E1E1E;
}
/*纯文本模板样式*/
.article-content-text{
  display: block;
  /* font-size: 100%; */
  margin: 0 15px 10px;
  line-height: 1.8;
  text-indent: 2em;
  font-size:14px;
}
/*引言模板样式*/
.article-content-quote{
  display: block;
  margin: 30px 0;
  text-align: center;
}
.article-content-quote-imgtop,.article-content-quote-imgbottom{
  display: block;
  width: 19px;
  height: 13px;
  margin: 0 auto;
}
.article-content-quote-text{
  display: block;
  text-align: center;
  width: 75%;
  font-size: 18px;
  font-weight: 700;
  margin: 0 auto;
  padding: 30px 0;
}
/*第三方视频模板样式*/
.article-content-thirdparty-video{
  display: block;
  margin: 5px 15px 20px;
  position: relative;
}
.article-content-thirdparty-video-img{
  display: block;
  width: 100%;
}
.article-content-thirdparty-video-icon{
  display: block;
  position: absolute;
  top: 50%;
  left: 50%;
  background:url("data:image/png;base64,") no-repeat 0 0;
  width: 80px;
  height: 80px;
  margin-left: -40px;
  margin-top: -40px;
  background-size: 80px 80px;
}
/*相册模板*/
.article-content-album{
  display: block;
  margin:0 15px 20px; 
}
.article-content-album-img{
  display: block;
  width: 100%;
}
.article-content-album-text{
  display: block;
  text-align: center;
  width: 100%;
  font-size: 14px;
  margin: 40px 0;
  color: #db3a45;
}
/*图文模版，带角标图注 */
.article-content-text-image{
  margin:0 15px 20px;
}
.article-content-text-image-img{
  display: block;
  width: 100%;
}
.article-content-text-image-txt-box{
  /* height: 64px; */
}
.article-content-text-image-icon{
  position: relative;
  left: 15px;
  top: -15px;
  width: 15px;
  height: 15px;
  z-index: 10;
}
.article-content-text-image-txt{
  display: block;
  font-size: 14px;
  color: #1E1E1E;
  padding: 0 10px 20px;
  border-bottom: solid;
  border-bottom-color: #d2d2d2;
  border-bottom-width: 1px;
  margin-bottom: 25px;
  line-height: 1.5;
  margin-top:-10px;
}
/*GIF图片模版*/
.article-content-gif{
  display: block;
  /* margin:0 15px 20px; */
   width: 100%; 
}
/*标题模板*/
.article-content-title{
  display: block;
  text-align: left;
  margin:0 15px 17px;
}
.article-content-title-txt{
  font-size: 18px;
  line-height: 1.2;
  padding-bottom: 5px;
  border-bottom:3px solid #1e1e1e;
}
/* 小标题模板样式 */
.article-content-sub-title{
  display: block;
  /* width: 100%; */
  padding: 0 0 5px;
  font-size: 14px;
  font-weight: 700;
  margin: 0 15px;
  word-break: break-all;
  white-space: normal;
}
/* 文章内容渲染样式 */
.articleinfo-keytitle{
  font-size: 14px;
  margin: 10px 15px 15px;
}
.articleinfo-keytitle-title{
  display: block;
}
.articleinfo-keytitle-word{
  display: block;
  font-size: 20px;
  font-weight: 600;
  /* margin: 10px 0 0; */
}
/* 投票模板 */
.article-content-vote{
  display: block;
  text-align: center;
  width: 100%;
  font-size: 14px;
  margin: 20px 0;
  color: #db3a45;
}
/*评论样式*/
.article-comments-title{
  display: block;
  font-size: 18px;
  margin: 10px 15px;
  /* font-weight: 600; */
}
.article-comments-line{
  display: block;
  height:10px;
  background: #f0f0f0;
}
.article-comments-item{
  display: block;
  padding-bottom: 10px;
  margin-top:10px;
  overflow: hidden;
}
.comments-item-bottom{
  display: block;
  border-bottom: 1px solid #ececec;
  margin-left:10px;
  margin-right:10px;
}
.comments-item-otherinfo{
  overflow: hidden;
  margin: 0 15px;
}
.comments-item-otherinfo-left{
  display: block;
  float: left;
  width: 415rpx;
  font-size: 10px;
  height: 38px;
}
.comments-item-avatar{
  display:block;
  float:left;
  width: 34px;
  height: 34px;
  border-radius: 50%;
  margin-right: 8px;
}
.comments-item-nickname{
  display:block;
  float:left;
  font-size: 12px;
  /* line-height: 1.2; */
  height: 20px;
  width: 325rpx; 
  margin-top: 2px;
}
.comments-item-createtime{
  display:block;
  float:left;
  line-height: 1.2;
  height: 16px;
  color:#888;
}
.comments-item-otherinfo-right{
  display: block;
  float: right;
}
.comments-item-reply{
  display: inline-block;
  height: 37px;
  line-height: 38px;
  vertical-align: middle;
  font-size: 10px;
  color:#888;
}
.comments-item-replynum{
  color:#000;
  margin: 0 10px 0 5px;
}
.comments-item-praise{
  display: inline-block;
  height: 40px;
  line-height: 36px;
  vertical-align: middle;
  margin-left: 10px;
}
.comments-item-praiseicon{
  display: inline-block;
  width:32rpx;
  height:32rpx;
  /* margin-top: 10px; */
   vertical-align: -1px;
}
.comments-item-praisenum{
  margin-right: 5px;
  font-size: 10px;
}
.comments-item-content{
  display: block;
  margin: 10px 15px 0;
  font-size: 12px;
}
.comments-content-image-box{
  display: block;
  padding: 10px 15px 0;
   height: 90px; 
   overflow: hidden; 
  box-sizing: border-box;
   white-space: nowrap; 
}
.comments-item-content-image{
  display: inline-block;
  margin:0 20rpx 20rpx 0;
  width: 120rpx;
  height: 80px;
}
/* .content-image-right{
  margin:0 0 10px 0;
} */
.article-comments-nolist{
  display: block;
  font-size: 14px;
  margin: 0 15px 10px;
}
/* 自己的评论样式  */
.comments-self-left{
  display: block;
  float:left;
  margin-left:15px; 
  font-size: 14px;
  width: 596rpx;
}
.comments-self-right{
  display: block;
  float:right;
  margin-right:15px;
}
.comments-self-line{
  display: inline-block;
  vertical-align: 2px;
  margin: 0 8px;
  height:14px;
}
.comments-self-replynum{
  margin-right: 10px;
}
.comments-self-replyfont{
  display: inline-block;
  margin-right: 5px;
  color:#888;
}
.comments-self-content{
  margin-top:6px; 
  font-size: 16px;
}
.comments-self-praisefont{
  margin-right:5px; 
  color:#888;
}
.comments-self-avatar{
  display: inline-block;
  width: 34px;
  height: 34px;
  border-radius: 50%;
}
.article-comments-app-btn{
  display:block;
  width: 100%;
  text-align: center;
  height:100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  font-weight: bolder;
  color:#fff;
  background: #000;
  margin-top: 0;
  margin-bottom: 0;
  border-radius: 0;
  box-sizing: border-box;
  background: #db3944;
}
.article-comments-app-form{
  display:block;
  position: absolute;
  bottom:0;
  left:0;
  width: 100%;
  height: 100rpx;
  background: #db3944;
}
.article-comments-app-form.iponex{
  padding-bottom: 15px;
}
.article-comments-app-icon{
  display: inline-block;
  width: 35rpx;
  height:40rpx;
  margin-right:15rpx; 
  margin-top:-7rpx;
  vertical-align: middle;
}

/* 商品详情轮播图 */
.goods-picture{
  display: block;
  width: 100%;
  /* height: 750rpx; */
  position: relative;
  padding-bottom: 30rpx;
  border-bottom: 12rpx solid #f7f8ff;
  margin-bottom: 20rpx;
}
.goods-picture-swiper{
  width: 100%;
  height: 580rpx;
}
.goods-picture-swiper-item{
  width: 100%;
  height: 100%;
  display: block;
  padding-top:1px;
  box-sizing: border-box; 
}
.goods-picture-img{
  display: block;
  width: 510rpx;
  height: 510rpx;
  margin: 30rpx auto 0; 
}
.goods-picture-info{
  display: block;
  width: 690rpx;
  /* height: 180rpx; */
  margin: 0 auto;
}
.goods-name{
  font-size: 32rpx;
  line-height: 42rpx;
  /* max-height: 84rpx;
  overflow: hidden; */
  font-weight: bold;
  margin-bottom: 16rpx;
}
.goods-price{
  font-size: 32rpx;
  color:#db3a45;
  font-weight: bold;
}
.goods-picture-swiper-count{
  position: absolute;
  top:535rpx;
  right:50rpx;
  font-size: 24rpx;
  color: #888;
}

/* 购买方式 */
.goods-buy-style{
  position: fixed;
  bottom:0;
  left:0;
  width: 100%;
  height: 90rpx;
  background: #000;
  /* display: flex; */
  /* flex-direction: row;
  justify-content: center;
  align-items: center; */
  padding-top: 10rpx;
}
.goods-buy-style-index{
  /* width: 126rpx; */
  width: 26%;
  height: 74rpx;
  text-align: center;
  display: block;
  font-size: 24rpx;
  color:#666;
  /* margin-right: 25rpx; */
  float: left;
  
}
.goods-buy-style-index image{
  display: inline-block;
  width: 47rpx;
  height: 76rpx;
}
.goods-buy-style-collection,.goods-buy-style-method{
  /* width: 256rpx; */
  display: block;
  float: left;
  width: 37%;
  height: 80rpx;
}
.goods-buy-style-collection image,.goods-buy-style-method image{
  display: block;
  width: 256rpx;
  height: 100%;
}
.goods-buy-style-collection{
  /* margin-right: 25rpx; */
}

.iphone{
  padding-bottom: 35rpx;
}