page{
  width: 100%;
  height: 100%; 
}
.container{
  display: block;
  width: 100%;
  height: 100%; 

}
/* 用户信息  */
.user-info{
  display: block;
  width: 100%;
  height: 20%;
  position: relative;
  border-bottom:12rpx solid #f4f4f4;
  box-sizing: border-box;
}
.user-info.iphone{
  height: 21%;
}
/* .user-info.iphone .user-info-avatar{
  margin: 5.5% auto 2%;
} */
.user-author-bg{
  display: block;
  width: 100%;
  height: 100%;
}
.user-info-inner{
  width: 100%;
  /* height: 100%; */
  position: absolute;
  top:50%;
  left:0;
  padding-top: 1px;
  box-sizing: border-box;
  margin-top:-104rpx;
}
.user-info-avatar{
  display: block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  /* margin: 3.5% auto 1.5%; */
  margin: 0 auto;
}
.user-info-nickname{
  display: block;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color:#fff;
  height: 48rpx;
  line-height: 48rpx;
}

/* 转发得优惠 */
.forword{
  display: block;
  width: 100%;
  height: 71%;
  overflow: hidden;
  background: url(data:image/jpg;base64,) no-repeat center top;
  background-size: 100% 100%;
}
.forword.iphone{
  height: 70%;
}
 
.forword-inner{
  display: block;
  width: 690rpx;
  margin: 0 auto;
  height: 89%;
  padding-top: 22rpx;
  box-sizing: border-box;
  position: relative;
}
.forword-like{
  display: block;
  float: left;
  width: 496rpx;
  height: 54rpx;
  line-height: 54rpx;
  font-size: 24rpx;
  
}
.activity-description{
  display: block;
  float: right;
  width: 180rpx;
  height: 54rpx;
}
.forword-swiper{
  display: block;
  width: 100%;
  height: 60%;
  overflow: hidden;
  margin: 22rpx 0 19rpx;
  clear: both;
}
.forword-swiper-item{
  display: block;
  width: 100%;
  height: 100%;
}
.forword-swiper-item image{
  display: block;
  width: 100%;
  height: 100%;
}
.forword-support{
  display: block;
  width: 100%;
  height: 10%;
  border-left:4rpx solid #000;
  border-right:4rpx solid #000;
  padding: 10rpx 35rpx;
  box-sizing: border-box;
  margin-bottom: 18rpx;
}
.forword-avatar{
  display: block;
  float: left;
  width: 60rpx;
  height:60rpx;
  border-radius: 50%;
  margin-left: -10rpx;
}
.forword-support-num{
  display: block;
  float: right;
  font-size: 24rpx;
  width: 80rpx;
  text-align: right;
  font-weight: bold;
}
.forword-support-num text{
   display: block; 
}
/*  */
.forword-share{
  width: 100%;
  background: #fff;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50rpx;
  position: relative;
}
.forword-form{
  display: block;
  float: left;
  width: 50%;
  height: 100%;
  line-height: 80rpx;
  border-radius: 20rpx;
  text-align: center;
}
.forword.iphone .forword-share{
  height: 100rpx;
  line-height: 100rpx;
}
.forword.iphone .user-forword-program,.forword.iphone .user-forword-timeline{
  width:54rpx;
  height: 54rpx;
  vertical-align: -15rpx;
}
.forword.iphone .forword-form{
  line-height: 100rpx;
}
.forword.iphone .forword-form button{
  line-height: 100rpx; 
}
.user-forword-program,.user-forword-timeline{
  display: inline-block;
  width:50rpx;
  height: 50rpx;
  border-radius: 50%;
  vertical-align: -10rpx;
}
.forword-form button{
  display: inline-block;
  font-size: 28rpx;
  background: #fff;
  padding: 0;
  line-height: 80rpx; 
  /* line-height: 2.7; */
  background-color: #fff;
  border:0;
  vertical-align: top;
}
.forword-form button::after{
  border:0;
}
.forword.iphone .forword-share-line{
  top:25rpx;
}
.forword-share-line{
  position: absolute;
  top:15rpx;
  left:50%;
  width: 2rpx;
  height: 50rpx;
  background: #eee;
}
.activity-descriptioned{
  position: absolute;
  top:84rpx;
  left:0;
  width: 100%;
  height: 70rpx;
  z-index: 5;
}
.activity-descriptioned image{
  display: block;
  width: 100%;
  height: 100%;
}
.activity-descriptioned  text{
  position: absolute;
  left:0;
  bottom:0;
  color:#fff;
  font-size: 24rpx;
  width: 100%;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
}
.user-show-share{
  position: fixed;
  top:0;
  left:0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.8);
  padding-top: 1px;
  box-sizing: border-box;
  z-index: 20;
}
.user-show-share-img{
  display: block;
  width: 630rpx;
  height: 813rpx;
  margin:40rpx auto 26rpx;
}
.share-vote-text{
  display: block;
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color:#fff;
}
.share-vote-cha{
  display: block;
  width:60rpx;
  height: 60rpx;
  margin:26rpx auto 0;
}





.share-body{
  display: block;
  width: 100%;
  height: 100%;
}
.share-body .user-info{
  height:27%;
  border-bottom:0;
}
.share-body .user-info-avatar{
  width: 160rpx;
  height: 160rpx;
  /* margin: 40rpx auto 16rpx; */
}
.share-body .forword{
  height: 73%;
}
.share-praise{
  display: block;
  width: 690rpx;
  height: 80rpx;
  /* background: #c49a58; */
  border-radius: 10rpx;
}
.share-praise button{
  display: block;
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  border-radius: 10rpx;
  background: #c49a58;
  color:#fff;
  font-size: 32rpx;
  font-weight: bold;
  letter-spacing: 1rpx;
}
.share-praise text{
  display: block;
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  border-radius: 10rpx;
  background: transparent;
  color:#000;
  font-size: 32rpx;
  font-weight: bold;
  letter-spacing: 1rpx;
  box-sizing: border-box;
  border:2rpx solid #000;
  text-align: center;
}


/* 底部导航 */
.nav-bottom{
  position: fixed;
  z-index: 10;
  bottom:0;
  left:0;
  display: block;
  width: 100%;
  height: 100rpx;
  background: #000;
}
.nav-bottom-list{
  display: block;
  width: 25%;
  float: left;
  text-align: center;
  height: 100%;
  box-sizing: border-box;
   padding-top: 6rpx; 
  /* line-height: 100rpx; */
}
.nav-bottom-list image{
  display: block;
  /* vertical-align: middle; */
  margin: 0 auto;
}
.nav-bottom-list text{
  display: block;
  width: 100%;
  text-align: center;
  font-size:20rpx;
  color:#666;
  height: 34rpx;
  line-height: 18rpx;
}
.nav-bottom.iphone{
  padding-bottom: 50rpx;
  box-sizing: content-box;
}